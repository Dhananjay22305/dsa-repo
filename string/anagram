def anagram(s, t):
    if len(s) != len(t):
        return False
    count = {}
    for i in range(len(s)):
        count[s[i]] = count.get(s[i], 0) + 1
        count[t[i]] = count.get(t[i], 0) - 1
    for v in count.values():
        if v != 0:
            return False
    return True

print(anagram("listen", "silent"))
print(anagram("hello", "world"))
